#include <iostream>
using namespace std;

int main() {
    int nums[] = {4, 7, 2, 9, 3};
    int size = sizeof(nums) / sizeof(nums[0]);
    int sum = 0;

    // accumulate elements
    for (int i = 0; i < size; i++) {
        sum += nums[i];
    }

    cout << "sum of elements: " << sum << endl;

    return 0;
}
